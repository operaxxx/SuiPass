# Development Environment Configuration
# Used for local development with local Sui network

# Load base configuration
VITE_NODE_ENV=development

# Network Configuration
VITE_SUI_NETWORK=local
VITE_SUI_RPC_URL=http://127.0.0.1:9000
# Note: Package ID is not needed for local development
# VITE_SUI_PACKAGE_ID=

# Walrus Configuration (disabled for local development)
VITE_WALRUS_RPC_URL=http://127.0.0.1:9876
VITE_WALRUS_SITE_URL=http://127.0.0.1:9876
VITE_ENABLE_LOCAL_MODE=true

# Development-specific settings
VITE_ENABLE_DEBUG_MODE=true